{
  "name": "StackEdit Plus",
  "description": "In-browser Markdown editor",
  "version": "6.0.0",
  "manifest_version": 2,
  "container" : "GITHUB",
  "api_console_project_id" : "241271498917",
  "icons": {
    "16": "icon-16.png",
    "32": "icon-32.png",
    "64": "icon-64.png",
    "128": "icon-128.png",
    "256": "icon-256.png",
    "512": "icon-512.png"
  },
  "app": {
    "urls": [
      "https://stackedit.net/"
    ],
    "launch": {
      "web_url": "https://stackedit.net/app"
    }
  },
  "offline_enabled": true,
  "permissions": [
    "unlimitedStorage"
  ]
}
